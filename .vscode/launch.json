{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debug Default",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"program": "${file}"
		},
		{
			"name": "Python3 Debug OS X",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"program": "${file}",
			"pythonPath":"/Library/Frameworks/Python.framework/Versions/3.5/bin/python3",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		}
	]
}